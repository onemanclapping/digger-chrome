{
    "name": "digger",
    "description": "digger",
    "version": "0.0.1",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "https://www.discogs.com/*"
    ],
    "browser_action": {
        "default_title": "digger"
    },
    "manifest_version": 2,
    "externally_connectable": {
        "matches": ["https://onemanclapping.github.io/digger/*"]
    }
}